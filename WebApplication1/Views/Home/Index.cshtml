<div ng-app="myApp" ng-controller="CtrlStores">
    <table class="table table-striped table table-hover table table-condensed">
        <thead>
            <tr>
                <th>Name of store</th>
                <th>Address</th>
                <th>Operating Mode</th>
            </tr>
        </thead>
        <tr ng-repeat="x in myWelcome">
            <td id="{{x.Id}}"><a href="http://localhost:18540/Home/PageProducts/?id={{x.Id}}">{{x.NameOfStore}}</a></td>
            <td>{{x.Addres }}</td>
            <td>{{x.OperatingMode }}</td>
        </tr>
    </table>

</div>
@section scripts
{
    <script src="https://code.angularjs.org/1.2.6/angular.js"></script>
    <script>
        angular.module("myApp", []);
    </script>
    <script src="~/Angular/Controller/CtrlStores.js"></script>
}
